/* pages/likedetail/likedetail.wxss */
@import "/wxParse/wxParse.wxss";
swiper image{
  width: 100%;
  height: 100%
}
.likedetail{
  background: #f4f4f4
}
.title{
  background: #ffffff;
  height: 80rpx;
  line-height: 80rpx;
  text-indent: 20rpx
}
.main{
  height: calc(100% - 100rpx)
}
.bottom{
  height: 100rpx;
  display: flex;
  align-items: center;
  bottom: 0;
  background: #ffffff
}
.bottom view{
  flex: 1;
  display: flex;
  justify-content: center;
  height: 100rpx;
  line-height: 100rpx
}
.bottom .yellow{
  background: #FF9222;
  color: #ffffff
}
.box{
  display: flex;
  align-items: center;
  width: 100%;
  background: #ffffff;
  border-bottom: 1rpx solid #f4f4f4
}
.text{
  flex: 1;
  text-align: right;
  margin-right: 20rpx
}
swiper{height: 550rpx}